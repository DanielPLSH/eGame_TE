<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
                         "http://castor.org/mapping.dtd">
<mapping>
	<class name="com.mpos.lottery.te.merchant.web.IncomeBalanceDto">
		<map-to xml="IncomeBalance" />

		<field name="payoutBalance" type="java.math.BigDecimal" required="true">
			<bind-xml name="payoutBalance" node="attribute" />
		</field>
		<field name="saleBalance" type="java.math.BigDecimal" required="true">
			<bind-xml name="saleBalance" node="attribute" />
		</field>
		<field name="cashoutBalance" type="java.math.BigDecimal" required="true">
			<bind-xml name="cashoutBalance" node="attribute" />
		</field>
		<field name="commissionBalance" type="java.math.BigDecimal" required="true">
			<bind-xml name="commissionBalance" node="attribute" />
		</field>
		
	</class>
</mapping>

