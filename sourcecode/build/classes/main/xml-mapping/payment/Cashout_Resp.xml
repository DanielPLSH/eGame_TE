<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
                         "http://castor.org/mapping.dtd">
<mapping>
	<class name="com.mpos.lottery.te.thirdpartyservice.playeraccount.web.CashoutResponse">
		<map-to xml="CashOut" />

		<field name="mobile" type="java.lang.String" required="true">
			<bind-xml name="accountId" node="attribute" />
		</field>
		<field name="leftCashoutAmount" type="java.math.BigDecimal"
			required="true">
			<bind-xml name="balance" node="attribute" />
		</field>
		<field name="cashoutAmount" type="java.math.BigDecimal"
			required="true">
			<bind-xml name="amount" node="attribute" />
		</field>
	</class>

</mapping>