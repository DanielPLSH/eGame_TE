<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
                         "http://castor.org/mapping.dtd">
<mapping>
	<class name="com.mpos.lottery.te.gameimpl.union.sale.UnionTicket">
		<map-to xml="Ticket" />

		<field name="rawSerialNo" type="java.lang.String" required="false">
			<bind-xml name="serialNo" node="attribute" />
		</field>
        <field name="barcode" type="java.lang.String" required="false">
            <bind-xml name="barcode" node="attribute" />
        </field>		
	</class>

</mapping>